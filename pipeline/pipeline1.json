{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "CopyNewFiles",
				"description": "Copy new and changed files only by using LastModifiedDate",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"modifiedDatetimeStart": {
								"value": "@adddays(utcnow(), -7)",
								"type": "Expression"
							},
							"modifiedDatetimeEnd": {
								"value": "@utcnow()",
								"type": "Expression"
							},
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "a",
									"type": "String"
								},
								"sink": {
									"name": "a",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "b",
									"type": "String"
								},
								"sink": {
									"name": "b",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "c",
									"type": "String"
								},
								"sink": {
									"name": "c",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "adlsgen2csv",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "adlsgen2parquet",
						"type": "DatasetReference",
						"parameters": {
							"FolderName": {
								"value": "@concat(utcNow('yyyy'), '-', utcNow('MM'))",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"annotations": []
	}
}