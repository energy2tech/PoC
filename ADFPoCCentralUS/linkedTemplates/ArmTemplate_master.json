{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFPoCCentralUS"
		},
		"ADB_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ADB'"
		},
		"AzureBatch_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatch'"
		},
		"AzureSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDB'"
		},
		"AzureSQLDB_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureSQLDB'"
		},
		"AzureSQLDB_ManagedId_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDB_ManagedId'"
		},
		"AzureSQLDB_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDB_SQL'"
		},
		"BatchPoolStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BatchPoolStorage'"
		},
		"ADLSGEN2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2poccus.dfs.core.windows.net"
		},
		"AzureBatch_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://pocbatch.centralus.batch.azure.com"
		},
		"AzureBatch_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "pocbatchpool"
		},
		"AzureBatch_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "pocbatch"
		},
		"AzureSQLDB_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "ebd8bf29-95be-4601-ae48-6723a463ae97"
		},
		"AzureSQLDB_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"rest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://dummy.restapiexample.com/api/v1/employees"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFPoCCentralUS_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADB_accessToken": {
						"value": "[parameters('ADB_accessToken')]"
					},
					"AzureSQLDB_connectionString": {
						"value": "[parameters('AzureSQLDB_connectionString')]"
					},
					"AzureSQLDB_servicePrincipalKey": {
						"value": "[parameters('AzureSQLDB_servicePrincipalKey')]"
					},
					"AzureSQLDB_ManagedId_connectionString": {
						"value": "[parameters('AzureSQLDB_ManagedId_connectionString')]"
					},
					"AzureSQLDB_SQL_connectionString": {
						"value": "[parameters('AzureSQLDB_SQL_connectionString')]"
					},
					"BatchPoolStorage_connectionString": {
						"value": "[parameters('BatchPoolStorage_connectionString')]"
					},
					"ADLSGEN2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGEN2_properties_typeProperties_url')]"
					},
					"AzureSQLDB_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureSQLDB_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureSQLDB_properties_typeProperties_tenant": {
						"value": "[parameters('AzureSQLDB_properties_typeProperties_tenant')]"
					},
					"rest_properties_typeProperties_url": {
						"value": "[parameters('rest_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFPoCCentralUS_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBatch_accessKey": {
						"value": "[parameters('AzureBatch_accessKey')]"
					},
					"AzureBatch_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch_properties_typeProperties_batchUri')]"
					},
					"AzureBatch_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatch_properties_typeProperties_poolName')]"
					},
					"AzureBatch_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch_properties_typeProperties_accountName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFPoCCentralUS_ArmTemplate_0')]"
			]
		},
		{
			"name": "ADFPoCCentralUS_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFPoCCentralUS_ArmTemplate_1')]"
			]
		}
	]
}