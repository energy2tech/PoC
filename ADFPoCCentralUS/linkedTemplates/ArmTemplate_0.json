			"name": "[concat(parameters('factoryName'), '/carrier_sql')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureSQLDB_ManagedId",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "item",
						"type": "varchar"
					},
					{
						"name": "datestring",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "input_date"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureSQLDB_ManagedId')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/cat_delete')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ADLSGEN2",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Excel",
				"typeProperties": {
					"sheetName": "Sheet1",
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "delete.xlsx",
						"fileSystem": "caterpillar"
					},
					"firstRowAsHeader": true
				},
				"schema": []
			},
			"dependsOn": [
